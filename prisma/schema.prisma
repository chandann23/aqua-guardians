// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Lake {
  id          Int      @id @default(autoincrement())
  name        String
  ph          Float
  temperature Float
  tds         Float
  turbidity   Float
  location    String   // URL pointing to the location of the lake
}